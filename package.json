    "node-sass": "^3.8.0",
    "nodemon": "^1.9.2",
    "npm-run-all": "^2.2.2",
    "open": "0.0.5",
    "react-addons-test-utils": "^15.1.0",
    "redux-immutable-state-invariant": "^1.2.3",
    "redux-mock-store": "^1.1.1",
    "rimraf": "^2.5.2",
    "sass-loader": "^3.2.2",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.1",
